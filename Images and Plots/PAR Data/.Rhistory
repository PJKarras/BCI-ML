file_name <- "merged2_PAR_Analysis_test_data_DAYTIME_13-446994955314361.csv"
data <- read.csv(file = file_name)
t_score <- data$PARDIFF / (data$std/(sqrt(data$count)))
t_score <- data$PARDIFF / (data$std)
boxplot(data$PARDIFF,main="Distribution of PARDIFF Values in Dataset",xlab="PARDIFF",horizontal = TRUE)
hist(data$PARDIFF, main="Distribution of PARDIFF Values for Full Dataset", xlab="PARDIFF", freq=TRUE)
plot(data$PAR, data$GPP, main="PAR vs GPP",cex.main=1.95,cex=0.8,cex.axis=2,cex.lab=1.7, xlab="PAR", ylab="GPP")
plot(t_score, data$gppdiff, main="PAR T-score vs GPP Error",cex.main=1.95,cex=0.8,cex.axis=2,cex.lab=1.7, xlab="PAR T-Score", ylab="GPP Error")
plot(t_score, data$gppdiff, main="PAR Z-score vs GPP Error",cex.main=1.95,cex=0.8,cex.axis=2,cex.lab=1.7, xlab="PAR T-Score", ylab="GPP Error")
cor(t_score, data$gppdiff)**2
plot(data$PAR, data$gppdiff, main="PAR Z-score vs GPP Error",cex.main=1.95,cex=0.8,cex.axis=2,cex.lab=1.7, xlab="PAR T-Score", ylab="GPP Error")
plot(data$PAR, data$gppdiff, main="PAR Z-score vs GPP Error",cex.main=1.95,cex=0.8,cex.axis=2,cex.lab=1.7, xlab="PAR", ylab="GPP Error")
cor(data$PAR, data$gppdiff)**2
plot(data$PAR, data$gppdiff, main="PAR vs GPP Error",cex.main=1.95,cex=0.8,cex.axis=2,cex.lab=1.7, xlab="PAR", ylab="GPP Error")
